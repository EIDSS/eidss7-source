@model EIDSS.Domain.ViewModels.Administration.DataArchivingViewModel
@{
    ViewData["Title"] = Localizer.GetString(HeadingResourceKeyConstants.DataArchivingSettingsPageHeading);
}
<div class="card">
    <div class="card-header">
        <h3>Data Archiving </h3>
    </div>
    <div class="card-body">
        <div class="col-sm-12">
            <h4></h4>
            <div class="form-group row ">
                <div class="form-group row col-md-12">

                   
                    <label class="col-md-3" field-label-for="@FieldLabelResourceKeyConstants.DataArchivingSettingsScheduleSummaryDescriptionFieldLabel"></label>
                    <div class="col-md-5">
                        <input type="text" value="@Model.archiveScheduledStartTime"
                               readonly class="form-control">
                    </div>
                </div>
                <div class="form-group row col-md-12">
                    <label class="col-md-3" field-label-for="@FieldLabelResourceKeyConstants.DataArchivingSettingsIntervalOfDataRelevanceInYearsFieldLabel"></label>
                    <div class="col-md-5">

                        <input type="text" value=@Model.dataAgeforArchiveInYears readonly class="form-control">
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>