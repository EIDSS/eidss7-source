@using Microsoft.AspNetCore.Mvc.TagHelpers
@model EIDSS.Domain.ViewModels.CrossCutting.AggregateReportGetListViewModel

<div class='row'>
    <div class='col-lg-6 col-md-6 col-sm-6 col-xs-6'>
