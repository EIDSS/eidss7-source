        <input type='text' class='form-control' disabled value="@Model.SentByOrganizationName" />
    </div>
    <div class='col-lg-6 col-md-6 col-sm-6 col-xs-6'>
        <label asp-for="@Model.SentByDate" field-label-for="@FieldLabelResourceKeyConstants.SearchHumanAggregateDiseaseReportsDateSentFieldLabel" class="control-label"></label>
        <input type='text' class='form-control' disabled value="@Model.SentByDate" />
    </div>
</div>
<div class='row'>
    <div class='col-lg-6 col-md-6 col-sm-6 col-xs-6'>
